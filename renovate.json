{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": [
    "regex"
  ],
  "labels": [
    "dependencies"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "Dockerfile"
      ],
      "matchStrings": [
        "ARG ALLURE_VERSION=(?<currentValue>.*?)\\n"
      ],
      "depNameTemplate": "allure-framework/allure2",
      "datasourceTemplate": "github-releases"
    }
  ]
}
