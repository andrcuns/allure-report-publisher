{
  "name": "allure-report-publisher",
  "description": "Publish allure 3 reports to cloud storage providers",
  "version": "5.0.0-alpha.3",
  "author": "Andrejs Cunskis",
  "bin": {
    "allure-report-publisher": "bin/run.js"
  },
  "bugs": "https://github.com/andrcuns/allure-report-publisher/issues",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@gitbeaker/rest": "^43.8.0",
    "@google-cloud/storage": "^7.18.0",
    "@oclif/core": "^4",
    "allure": "^3.0.0",
    "chalk": "^5.0.0",
    "cli-table3": "^0.6.0",
    "glob": "^13.0.0",
    "mime-types": "^3.0.0",
    "nano-spawn": "^2.0.0",
    "ora": "^9.0.0",
    "p-all": "^5.0.1",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.1.15",
    "@types/chai": "^5.2.3",
    "@types/mime-types": "^3.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "allure-mocha": "^3.4.3",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "eslint-config-oclif": "^6.0.128",
    "eslint-config-prettier": "^10.1.8",
    "mocha": "^11.7.5",
    "oclif": "^4.22.61",
    "shx": "^0.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/andrcuns/allure-report-publisher",
  "keywords": [
    "oclif",
    "allure",
    "test-report"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "allure-report-publisher",
    "dirname": "allure-report-publisher",
    "commands": "./dist/commands",
    "plugins": [],
    "topicSeparator": " ",
    "topics": {
      "upload": {
        "description": "Upload reports to cloud storage or CI artifacts"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrcuns/allure-report-publisher.git"
  },
  "scripts": {
    "build": "tsc -b",
    "cleanBuild": "shx rm -rf dist tsconfig.tsbuildinfo && tsc -b",
    "lint": "eslint",
    "prepack": "oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prereadme": "shx rm -rf dist tsconfig.tsbuildinfo && tsc -b",
    "readme": "oclif readme"
  },
  "types": "dist/index.d.ts"
}
